    <VisualElement name="root" class="root">
        <VisualElement class="center-container panel centered-panel">
            <VisualElement name="centerContainer" class="center-container">
                <VisualElement name="hud" class="hud">
                    <VisualElement name="actions" class="actions">
                        <Button name="restButton" text="Rest" style="background-color: rgb(81, 182, 22);" />
                        <Button name="practiseButton" text="Practise" style="background-color: rgb(188, 114, 73);" />
                        <Button name="gigButton" text="Gig" style="background-color: rgb(138, 49, 49);" />
                    </VisualElement>
                    <VisualElement class="day-container">
                        <Label name="dayLabel" text="Day 1 / 1" class="day-label" />
                    </VisualElement>
                    <VisualElement name="topStats" class="top-stats">
                        <VisualElement class="stat">
                            <Label text="Energy" class="stat-label" />
                            <ProgressBar name="energyBar" class="stat-bar energy" />
                        </VisualElement>
                        <VisualElement class="stat">
                            <Label text="Skill" class="stat-label" />
                            <ProgressBar name="skillBar" class="stat-bar skill" />
                        </VisualElement>
                        <VisualElement class="stat">
                            <Label text="Popularity" class="stat-label" />
                            <ProgressBar name="popularityBar" class="stat-bar popularity" />
                        </VisualElement>
                        <VisualElement class="stat">
                            <Label text="Fame" class="stat-label" />
                            <ProgressBar name="fameBar" class="stat-bar fame" />
                        </VisualElement>
                    </VisualElement>
                </VisualElement>
            </VisualElement>
        </VisualElement>
    </VisualElement>
